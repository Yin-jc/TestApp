+        // 通过ContentResolver 向ContentProvider中查询数据
+        Cursor cursor = resolver.query(uri_user, new String[]{"_id","name"}, null, null, null);
+        while (cursor.moveToNext()){
+            System.out.println("query book:" + cursor.getInt(0) +" "+ cursor.getString(1));
